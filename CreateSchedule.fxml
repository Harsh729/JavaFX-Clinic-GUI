<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateScheduleController">
   <children>
      <Label layoutX="157.0" layoutY="14.0" text="Create Schedule" />
      <Label layoutX="53.0" layoutY="92.0" text="Date:" />
      <DatePicker fx:id="date" layoutX="87.0" layoutY="88.0" />
      <ButtonBar layoutY="160.0" prefHeight="40.0" prefWidth="400.0">
        <buttons>
          <Button fx:id="CancelButton" mnemonicParsing="false" onAction="#closeWindow" text="Cancel" />
            <Button fx:id="OKButton" mnemonicParsing="false" onAction="#save" text="OK" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
   </children>
</AnchorPane>
